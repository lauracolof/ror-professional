<p>Intentos: <%= session[:intentos] %> </p>
<form action=<%= photos_path %>  method="post">
  <%= hidden_field_tag :authenticity_token, form_authenticoty_token %>

    <div>
      <label for="title"> Title</label>
      <input type="text" name="photo[title]" id="title">
    </div>
    <div>
      <label for="image_url"> Image URL</label>
      <input type="url" name="photo[image_url]" id="image_url">
    </div>

  <input type="submit" value="Send">

</form>